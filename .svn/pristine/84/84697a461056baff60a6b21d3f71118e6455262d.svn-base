package com.tly.model;

import java.util.Date;

/**
 * 故障设备维修明细
 * @ClassName EquipmentMaintenance
 * @Author yangweihang
 * @Date 2018年8月26日 下午11:30:17
 */
public class EquipmentMaintenance {
	private Integer emid;//id
	private Integer pid;//合伙人id
	private Integer wid;//维修人员id
	private Integer eid;//设备id
	private String describe;//故障描述
	private String judge;//工程师判断
	private String record;//维修记录
	private String result;//维修结果
	private Date startime;//维修开始时间
	private Date endtime;//维修结束时间
	public EquipmentMaintenance() {
	}
	public EquipmentMaintenance(Integer emid, Integer pid, Integer wid, Integer eid, String describe, String judge,
			String record, String result, Date startime, Date endtime) {
		this.emid = emid;
		this.pid = pid;
		this.wid = wid;
		this.eid = eid;
		this.describe = describe;
		this.judge = judge;
		this.record = record;
		this.result = result;
		this.startime = startime;
		this.endtime = endtime;
	}
	public Integer getEmid() {
		return emid;
	}
	public void setEmid(Integer emid) {
		this.emid = emid;
	}
	public Integer getPid() {
		return pid;
	}
	public void setPid(Integer pid) {
		this.pid = pid;
	}
	public Integer getWid() {
		return wid;
	}
	public void setWid(Integer wid) {
		this.wid = wid;
	}
	public Integer getEid() {
		return eid;
	}
	public void setEid(Integer eid) {
		this.eid = eid;
	}
	public String getDescribe() {
		return describe;
	}
	public void setDescribe(String describe) {
		this.describe = describe;
	}
	public String getJudge() {
		return judge;
	}
	public void setJudge(String judge) {
		this.judge = judge;
	}
	public String getRecord() {
		return record;
	}
	public void setRecord(String record) {
		this.record = record;
	}
	public String getResult() {
		return result;
	}
	public void setResult(String result) {
		this.result = result;
	}
	public Date getStartime() {
		return startime;
	}
	public void setStartime(Date startime) {
		this.startime = startime;
	}
	public Date getEndtime() {
		return endtime;
	}
	public void setEndtime(Date endtime) {
		this.endtime = endtime;
	}
	@Override
	public String toString() {
		return "EquipmentMaintenance [emid=" + emid + ", pid=" + pid + ", wid=" + wid + ", eid=" + eid + ", describe="
				+ describe + ", judge=" + judge + ", record=" + record + ", result=" + result + ", startime=" + startime
				+ ", endtime=" + endtime + "]";
	}
}
