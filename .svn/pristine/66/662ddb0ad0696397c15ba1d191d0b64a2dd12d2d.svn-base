package com.tly.model;


/**
 * 设备子表
 * @ClassName EquipmentSublist
 * @Author yangweihang
 * @Date 2018年8月24日 上午11:23:54
 */
public class EquipmentSublist {
	private Integer id;//id
	private Integer eid;//设备表id
	private String number;//设备编号
	private String ewm;//二维码
	private Double cashpledge;//押金
	private Double price;//售价
	private Double platformprice;//平台价格(25/次)
	private Integer servicetime;//使用时长
	public EquipmentSublist() {
	}
	public EquipmentSublist(Integer id, Integer eid, String number, String ewm, Double cashpledge, Double price,
			Double platformprice, Integer servicetime) {
		this.id = id;
		this.eid = eid;
		this.number = number;
		this.ewm = ewm;
		this.cashpledge = cashpledge;
		this.price = price;
		this.platformprice = platformprice;
		this.servicetime = servicetime;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Integer getEid() {
		return eid;
	}
	public void setEid(Integer eid) {
		this.eid = eid;
	}
	public String getNumber() {
		return number;
	}
	public void setNumber(String number) {
		this.number = number;
	}
	public String getEwm() {
		return ewm;
	}
	public void setEwm(String ewm) {
		this.ewm = ewm;
	}
	public Double getCashpledge() {
		return cashpledge;
	}
	public void setCashpledge(Double cashpledge) {
		this.cashpledge = cashpledge;
	}
	public Double getPrice() {
		return price;
	}
	public void setPrice(Double price) {
		this.price = price;
	}
	public Double getPlatformprice() {
		return platformprice;
	}
	public void setPlatformprice(Double platformprice) {
		this.platformprice = platformprice;
	}
	public Integer getServicetime() {
		return servicetime;
	}
	public void setServicetime(Integer servicetime) {
		this.servicetime = servicetime;
	}
	@Override
	public String toString() {
		return "EquipmentSublist [id=" + id + ", eid=" + eid + ", number=" + number + ", ewm=" + ewm + ", cashpledge="
				+ cashpledge + ", price=" + price + ", platformprice=" + platformprice + ", servicetime=" + servicetime
				+ "]";
	}
}
