package com.tly.model;


/**
 * 合伙人级别
 * @ClassName PartnerLevel
 * @Author yangweihang
 * @Date 2018年8月24日 上午11:25:47
 */
public class PartnerLevel {
	private int id;//id
	private String name;//合伙人级别名称  1.普通 2.金牌 3.钻石 4.超级
	public PartnerLevel() {
	}
	public PartnerLevel(int id, String name) {
		this.id = id;
		this.name = name;
	}
	@Override
	public String toString() {
		return "PartnerLevel [id=" + id + ", name=" + name + "]";
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
